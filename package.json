{
  "name": "getserved-socket",
  "version": "3.3.0-socket",
  "description": "Websocket for getserved",
  "main": "build/index.js",
  "scripts": {
    "start": "NODE_ENV=production node build/index.js",
    "test:unit": "jest",
    "test:preformance": "npx artillery run socketio.test.yml",
    "pm2:start": "npx pm2 start ecosystem.config.js",
    "pm2:plus": "npx pm2 plus",
    "pm2:monit": "npx pm2 monit",
    "pm2:status": "npx pm2 status",
    "pm2:restart": "npx pm2 restart ecosystem.config.js",
    "pm2:stop": "npx pm2 stop ecosystem.config.js",
    "pm2:delete": "npx pm2 delete ecosystem.config.js",
    "pm2:monitor": "npx pm2 monitor",
    "pm2:logs": "npx pm2 logs",
    "dev": "NODE_ENV=development tsnd --respawn --transpile-only src/index.ts",
    "build": "tsc --build",
    "dev:test:default:socket": "tsnd --respawn --transpile-only test-manual/socket.io-client/default.test/index.test.ts"
  },
  "keywords": [
    "getserved",
    "socket"
  ],
  "license": "ISC",
  "dependencies": {
    "@socket.io/redis-adapter": "^7.2.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "dayjs": "^1.11.4",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.0",
    "pino": "^8.3.1",
    "pino-pretty": "^8.1.0",
    "pm2": "^5.2.0",
    "redis": "^4.2.0",
    "socket.io": "^4.5.1",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.6.3",
    "@types/redis": "^4.0.11",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "artillery": "^2.0.0-22",
    "fs": "^0.0.1-security",
    "jest": "^28.1.3",
    "socket.io-client": "^4.5.1",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
