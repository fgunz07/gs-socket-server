{
  "name": "getserved-socket",
  "version": "4.0.0",
  "description": "Websocket for getserved",
  "main": "build/index.js",
  "scripts": {
    "dev": "nodemon -L",
    "test": "jest --watch"
  },
  "keywords": [
    "getserved",
    "socket"
  ],
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.1",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "nodemonConfig": {
    "watch": "./src/index.ts",
    "ext": "ts",
    "ignore": [
      "*.test.ts",
      "node_modules/**/*"
    ],
    "exec": "ts-node --transpileOnly ./src/index.ts",
    "env": {
      "NODE_ENV": "development",
      "NODE_PORT": 3000
    }
  }
}
